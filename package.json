{
  "name": "connect-harperdb",
  "version": "0.0.3-beta",
  "description": "HarperDB session store for ExpressJS",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "scripts": {
    "prepublishOnly": "npm run build",
    "build": "tsc -p tsconfig.json",
    "fix:prettier": "prettier \"lib/**/*.ts\" --write",
    "test": "nyc ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Lucky-victory/connect-harperdb.git"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "harperdb",
    "session",
    "store",
    "connect",
    "express session"
  ],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Lucky-victory/connect-harperdb/issues"
  },
  "homepage": "https://github.com/Lucky-victory/connect-harperdb#readme",
  "devDependencies": {
    "@ava/typescript": "^4.0.0",
    "@types/debug": "^4.1.7",
    "@types/express": "^4.17.17",
    "@types/express-session": "^1.17.7",
    "@types/node": "^18.15.5",
    "@types/supertest": "^2.0.12",
    "ava": "^5.2.0",
    "debug": "^4.3.4",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "nyc": "^15.1.0",
    "prettier": "^2.8.6",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "harpee": "^4.0.11"
  },
  "prettier": {
    "singleQuote": true,
    "semi": true,
    "trailingComma": "es5"
  },
  "peerDependencies": {
    "express-session": "^1.17.3"
  },
  "ava": {"files": [
      "!dist/lib/**",
      "!src/test/helper.ts"
    ],
    "typescript": {
      "rewritePaths": {
        "src/": "dist/"
      },
      "compile": false
    }
  }
